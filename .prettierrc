{
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular",
        "bracketSameLine": true,
        "singleAttributePerLine": false,
        "printWidth": 200
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "arrowParens": "avoid",
        "singleQuote": true,
        "printWidth": 140
      }
    }
  ]
}
